<h1><%= @user.username %>'s' Logs</h1>
<%= render "/flash" %>

<h3>Games</h3>
	<ul>
	<% @games.each do |game| %>
		<%= print_item(game) %><br>
	<% end %>
	</ul>
<h3>Movies</h3>
	<ul>
	<% @movies.each do |movie| %>
		<%= print_item(movie) %><br>
	<% end %>
	</ul>
<h3>Books</h3>
	<ul>
	<% @books.each do |book| %>
		<%= print_item(book) %><br>
	<% end %>
	</ul>
	

